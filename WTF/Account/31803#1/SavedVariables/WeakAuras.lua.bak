["init"] = {
["custom"] = "--categoryNumber can be used to differentiate between casts in the conditions for coloring etc\n\n--[spellID] = {\"custom name\",category,\"show (true/false)\",\"soundFile\"},\naura_env.spellIDs = {\n    \n    --Ara-Kara\n    \n    [438471] = {\"Buster\", 4, true, \"Bite.ogg\"}, --Voracious Bite\n    --[433002] = {\"Leech\", 0, true, \"\"}, --Extraction Strike\n    --[433785] = {\"Buster\", 0, true, \"\"}, --Grasping Slash\n    \n    --City of Threads\n    \n    --[443397] = {\"Buster\", 0, true, \"\"}, --Venom Strike\n    [461842] = {\"Buster\", 4, true, \"Bite.ogg\"}, --Oozing Smash\n    [439646] = {\"Buster\", 0, true, \"Bite.ogg\"}, --Process of Elimination\n    --[440468] = {\"Buster\", 0, true, \"\"}, --Rime Dagger\n    --[439621] = {\"Frontal\", 1, true, \"\"}, --Shade Slash\n    [434722] = {\"Buster\", 4, true, \"Bite.ogg\"}, --Subjugate\n    \n    --Grim Batol\n    \n    [451241] = {\"Buster\", 0, true, \"\"}, --Shadowflame Slash\n    --[462216] = {\"Frontal\", 1, true, \"\"}, --Blazing Shadowflame\n    --[451364] = {\"Buster\", 0, true, \"\"}, --Brutal Strike\n    [450100] = {\"Knock\", 4, true, \"Knock.ogg\"}, --Crush\n    [451971] = {\"Buster\", 4, true, \"\"}, --Lava Fist\n    [449444] = {\"Buster\", 4, true, \"Bite.ogg\"}, --Molten Flurry\n    [451378] = {\"Buster\", 0, true, \"\"}, --Rive\n    [447261] = {\"Buster\", 4, true, \"Bite.ogg\"}, --Skullsplitter\n    \n    --Mists\n    \n    --[322557] = {\"Buster\", 0, true, \"\"}, --Soul Split\n    --[463256] = {\"Pools\", 2, true, \"\"}, --Mist Ward\n    [463217] = {\"Buster\", 0, true, \"\"}, --Anima Slash\n    --[324986] = {\"Bite\", 0, true, \"\"}, --Mistveil Bite\n    [321828] = {\"Kick\", 3, true, \"Kick.ogg\"}, --Patty Cake\n    \n    --Boralus\n    \n    [256709] = {\"Buster\", 0, true, \"\"}, --Singing Steel\n    --[268230] = {\"Buster\", 0, true, \"\"}, --Crimson Swipe\n    --[272588] = {\"Healreduce\", 0, true, \"\"}, --Rotting Wounds\n    --[275835] = {\"Poison\", 0, true, \"\"}, --Stinging Venom Coating\n    --[269029] = {\"Frontal\", 1, true, \"\"}, --Clear the Deck\n    --[257288] = {\"Frontal\", 1, true, \"\"}, --Heavy Slash\n    --[256627] = {\"Frontal\", 1, true, \"\"}, --Slobber Knocker\n    \n    \n    --Dawnbreaker\n    \n    [431637] = {\"Buster\", 0, true, \"\"}, --Umbral Rush\n    --[431493] = {\"Buster\", 0, true, \"\"}, --Darkblade\n    --[453212] = {\"Buster\", 0, true, \"\"}, --Obsidian Beam\n    [431491] = {\"Buster\", 0, true, \"\"}, --Tainted Slash\n    [451117] = {\"Buster\", 0, true, \"Knock.ogg\"}, --Terrifying Slam\n    [427001] = {\"Buster\", 0, true, \"Knock.ogg\"}, --Terrifying Slam\n    \n    --Necrotic Wake\n    \n    [320696] = {\"Buster\", 0, true, \"\"}, --Bone Claw\n    [321807] = {\"Buster\", 0, true, \"\"}, --Boneflay\n    [320655] = {\"Buster\", 0, true, \"\"}, --Crunch\n    --[324323] = {\"Frontal\", 1, true, \"\"}, --Gruesome Cleave\n    --[320771] = {\"Buster\", 0, true, \"\"}, --Icy Shard\n    [338456] = {\"Buster\", 0, true, \"\"}, --Multilate\n    [320376] = {\"Buster\", 0, true, \"\"}, --Multilate\n    --[338636] = {\"Buster\", 0, true, \"\"}, --Seperate Flesh\n    --[334488] = {\"Buster\", 0, true, \"\"}, --Sever Flesh\n    [338357] = {\"Buster\", 0, true, \"\"}, --Tenderize\n    \n    --Stonevault\n    \n    --[427382] = {\"Buster\", 0, true, \"\"}, --Concussive Smash\n    [422233] = {\"Buster\", 4, true, \"Bite.ogg\"}, --Crystalline Smash\n    [428711] = {\"Buster\", 4, true, \"Bite.ogg\"}, --Igneous Hammer\n    [424888] = {\"Buster\", 4, true, \"Bite.ogg\"}, --Seismic Smash\n    --[425027] = {\"Frontal\", 1, true, \"Front.ogg\"}, --Seismic Wave\n    [459210] = {\"Shadow Claw\", 0, true, \"\"}, --Shadow Claw\n    --[428894] = {\"Buster\", 0, true, \"\"}, --Stonebreaker Strike\n    \n}\n\n-- this is only for spells where the mobs cast it for roleplay purposes and they should only show if target is in party\naura_env.rpspellIDs = {\n    [431637] = true, --Umbral Rush\n}\n\n--[[\ndefault logic:\n- category 0 Tankbusters\n- category 1 Frontals\n- category 2 Pools\n- category 3 Special stuff\n- important Tankbusters\n]]\n\naura_env.getCastInfo = function(unit)\n    local name, startC, endC, icon, notInterruptible, spellID, duration, expirationTime, _, castType\n    if UnitCastingInfo(unit) then\n        name, _, icon, startC, endC, _, _, notInterruptible, spellID = UnitCastingInfo(unit)\n        castType = \"cast\"\n    elseif UnitChannelInfo(unit) then\n        name, _, icon, startC, endC, _, notInterruptible, spellID = UnitChannelInfo(unit)\n        castType = \"channel\"\n    end\n    if startC and endC then\n        duration = (endC - startC) / 1000\n        expirationTime = endC / 1000\n    end\n    return name, duration, expirationTime, icon, notInterruptible, spellID, castType\nend",
