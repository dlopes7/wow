["custom"] = "function(allstates, event, ...)\n    if event == \"COMBAT_LOG_EVENT_UNFILTERED\" then\n        local timestamp, subEvent, hideCaster, sourceGUID, sourceName, sourceFlags, sourceRaidFlags, destGUID, destName, destFlags, destRaidFlags, spellID, _,_, auraType = ...\n        if subEvent == \"SPELL_CAST_SUCCESS\" and spellID == 274991 then\n            aura_env.timestamp = GetTime()\n            local guid = UnitGUID(\"boss1target\")\n            if guid then\n                local unit = UnitTokenFromGUID(guid)\n                if unit then\n                    local role = UnitGroupRolesAssigned(unit)\n                    if role ~= \"TANK\" then\n                        allstates[guid] = {\n                            show = true,\n                            changed = true,\n                            progressType = \"timed\",\n                            duration = 2.5,\n                            expirationTime = 2.5 + GetTime(),\n                            unit = unit,\n                            autoHide = true,\n                        }\n                        return true\n                    end\n                end\n            end\n        elseif subEvent == \"SPELL_AURA_APPLIED\" and spellID == 275014 then\n            local state = allstates[destGUID]\n            if state then\n                state.show = false\n                state.changed = true\n                return true\n            end\n        end\n    elseif event == \"UNIT_TARGET\" and aura_env.timestamp and ((GetTime() - aura_env.timestamp) == 0) then\n        --sadly it's only possible to glow one of the two targeted players\n        local unit = ...\n        local guid = UnitGUID(unit..\"target\")\n        if guid and not allstates[guid] then\n            local unit = UnitTokenFromGUID(guid)\n            if unit then\n                allstates[guid] = {\n                    show = true,\n                    changed = true,\n                    progressType = \"timed\",\n                    duration = 2.5,\n                    expirationTime = 2.5 + GetTime(),\n                    unit = unit,\n                    autoHide = true,\n                }\n                return true\n            end\n        end\n    elseif event == \"ENCOUNTER_START\" then\n        aura_env.timestamp = nil\n    end\nend",
