["events"] = "UNIT_TARGET:nameplate, UNIT_SPELLCAST_START:nameplate, UNIT_SPELLCAST_SUCCEEDED:nameplate, UNIT_SPELLCAST_STOP:nameplate, UNIT_SPELLCAST_FAILED:nameplate, UNIT_SPELLCAST_INTERRUPTED:nameplate, CAUSESE_DELAYED_SPELLCAST",
["custom"] = "function(allstates, event, unit, _, spellID)\n    if (event == \"UNIT_TARGET\" or event == \"CAUSESE_DELAYED_SPELLCAST\")\n    and UnitExists(unit)\n    and UnitIsUnit(unit..\"target\", \"player\")\n    then\n        local _,_,icon,_,expirationTime,_,_,_,spellId = UnitCastingInfo(unit)\n        local guid = UnitGUID(unit)\n        if spellId\n        and aura_env.allowlist[spellId]\n        and guid\n        and not allstates[guid] then\n            local name, sound = unpack(aura_env.allowlist[spellId])\n            PlaySoundFile(\"Interface\\\\AddOns\\\\SharedMedia_Causese\\\\sound\\\\\"..sound, \"MASTER\")\n            allstates[guid] = {\n                show = true,\n                changed =  true,\n                progressType = \"timed\", \n                duration = expirationTime / 1000 - GetTime(),\n                expirationTime = expirationTime / 1000,\n                name = name,\n                icon = icon,\n                autoHide = true,\n                spellID = spellId\n            }\n            return true\n        end\n        \n    elseif event == \"UNIT_SPELLCAST_START\" and aura_env.allowlist[spellID] then\n        C_Timer.After(0.15, function() WeakAuras.ScanEvents(\"CAUSESE_DELAYED_SPELLCAST\", unit) end)\n        \n    elseif (\n        event == \"UNIT_SPELLCAST_SUCCEEDED\"\n        or event == \"UNIT_SPELLCAST_STOP\"\n        or event == \"UNIT_SPELLCAST_FAILED\"\n        or event == \"UNIT_SPELLCAST_INTERRUPTED\"\n    )    \n    and UnitExists(unit)\n    then\n        local guid = UnitGUID(unit)\n        if guid then\n            local state = allstates[guid]\n            if state then\n                state.show = false\n                state.changed = true\n                return true\n            end\n        end\n    end\nend",
