["init"] = {
["custom"] = "--categoryNumber can be used to differentiate between casts in the conditions for coloring etc\n--role can be (ALL/DAMAGER/HEALER/TANK/MELEE/RANGE) and only shows the bar to that role\n\n--[spellID] = {\"name\",category,\"soundFile\",\"role\", \"show target\" (true/false), \"important\" (true/false)},\naura_env.spellIDs = {\n    \n    --Boralus\n    \n    [257063] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Brackish Bolt\n    [272581] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Water Bolt\n    [454440] = {\"VOMIT\",1,\"\",\"ALL\",true,false}, --Stinky Vomit\n    [256639] = {\"BOMB\",2,\"\",\"ALL\",true,false}, --Fire Bomb\n    [257641] = {\"SLUG\",2,\"\",\"ALL\",true,false}, --Molten Slug\n    [272528] = {\"SHOT\",2,\"\",\"ALL\",true,false}, --Shoot\n    [272542] = {\"RICO\",2,\"\",\"ALL\",true,false}, --Ricochet\n    --[272546] = {\"BANANA\",2,\"\",\"ALL\",false,false}, --Banana Rampage\n    [256957] = {\"SHELL\",0,\"\",\"ALL\",false,true}, --Watertight Shell\n    [272571] = {\"CHOKE\",0,\"\",\"ALL\",true,true}, --Choking Waters\n    --[256709] = {\"BUSTER\",2,\"\",\"ALL\",true,false}, --Singing Steel (tank)\n    --[268230] = {\"BUSTER\",2,\"\",\"ALL\",true,false}, --Crimson Swipe (tank)\n    --[272588] = {\"BUSTER\",2,\"\",\"ALL\",true,false}, --Rotting Wounds (tank)\n    --[275835] = {\"BUSTER\",2,\"\",\"ALL\",true,false}, --Stinging Venom Coating (tank)\n    [275826] = {\"SHOUT\",0,\"\",\"ALL\",false,true}, --Bolstering Shout\n    \n    --Ara-Kara\n    \n    [438618] = {\"SPIT\",2,\"\",\"ALL\",true,false}, --Venomous Spit\n    [448248] = {\"VOLLEY\",0,\"\",\"ALL\",false,true}, --Revolting Volley\n    [433841] = {\"VOLLEY\",0,\"\",\"ALL\",false,true}, --Venom Volley\n    [448239] = {\"WEB\",0,\"\",\"ALL\",true,false}, --Web Wrap\n    [434802] = {\"FEAR\",0,\"\",\"ALL\",false,true}, --Horrifying Shrill\n    [434793] = {\"BARRAGE\",0,\"\",\"ALL\",false,true}, --Resonant Barrage\n    [434786] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Web Bolt\n    [432967] = {\"ALARM\",2,\"\",\"ALL\",false,true}, --Alarm Shrill\n    --[436322] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Poison Bolt (tank)\n    --[433002] = {\"BUSTER\",2,\"\",\"ALL\",true,false}, --Extraction Strike (tank)\n    \n    --Mists\n    \n    --[322557] = {\"BUSTER\",2,\"\",\"TANK\",true,false}, --Soul Split\n    [325418] = {\"DOT\",2,\"\",\"ALL\",true,false}, --Volatile Acid\n    [325223] = {\"LEAP\",2,\"\",\"ALL\",true,false}, --Anima Injection\n    [322767] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Spirit Bolt\n    [326319] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Spirit Bolt\n    [322938] = {\"HARVEST\",0,\"\",\"ALL\",true,true}, --Harvest Essence\n    [324776] = {\"SHIELD\",0,\"\",\"ALL\",false,true}, --Bramblethorn Coat\n    [324914] = {\"HEAL\",0,\"\",\"ALL\",false,true}, --Nourish the Forest\n    [326046] = {\"SHIELD\",0,\"\",\"ALL\",false,true}, --Stimulate Resistance\n    [340544] = {\"HEAL\",0,\"\",\"ALL\",false,true}, --Stimulate Regeneration\n    \n    --Dawnbreaker\n    \n    [431303] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Night Bolt\n    [451113] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Web Bolt\n    [431309] = {\"CURSE\",0,\"\",\"ALL\",true,true}, --Ensnaring Shadows\n    [450756] = {\"HOWL\",0,\"\",\"ALL\",false,true}, --Abyssal Howl\n    [431333] = {\"BEAM\",1,\"\",\"ALL\",true,true}, --Tormenting Beam\n    --[431637] = {\"BUSTER\",2,\"\",\"TANK\", true,false}, --Umbral Rush (tank)\n    [432520] = {\"SHIELD\",2,\"\",\"ALL\",false,true}, --Umbral Barrier\n    [451097] = {\"SHELL\",0,\"\",\"ALL\",false,true}, --Silken Shell\n    --[452099] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Congealed Darkness (tank)\n    \n    --Grim Batol\n    \n    [451261] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Earth Bolt\n    [76369] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Shadowflame Bolt\n    [451871] = {\"TREMOR\",0,\"\",\"ALL\",false,true}, --Mass Tremor\n    [76711] = {\"SEAR\",0,\"\",\"ALL\",true,true}, --Sear Mind\n    [451224] = {\"FLAME\",2,\"\",\"ALL\",true,true}, --Enveloping Shadowflame\n    [451241] = {\"BUSTER\",2,\"\",\"ALL\",true,false}, --Shadowflame Slash\n    \n    --Necrotic Wake\n    \n    [334747] = {\"THROW\",2,\"\",\"ALL\",true,false}, --Throw Flesh\n    [321780] = {\"ANIMATE\",2,\"\",\"ALL\",false,false}, --Animate Dead\n    --[327393] = {\"GRIM\",2,\"\",\"ALL\",false,false}, --Grim Fate\n    [334748] = {\"DRAIN\",0,\"\",\"ALL\",true,true}, --Drain Fluids\n    [320462] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Necrotic Bolt\n    [333602] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Frostbolt\n    [328667] = {\"VOLLEY\",0,\"\",\"ALL\",false,true}, --Frostbolt Volley\n    [333629] = {\"SHOOT\",2,\"\",\"ALL\",true,false}, --Shoot\n    [328687] = {\"SHOOT\",2,\"\",\"ALL\",true,false}, --Shoot\n    [335143] = {\"MEND\",1,\"\",\"ALL\",false,true}, --Bonemend\n    [327130] = {\"REPAIR\",0,\"\",\"ALL\",false,true}, --Repair Flesh\n    [324293] = {\"FEAR\",0,\"\",\"ALL\",false,true}, --Rasping Scream\n    --[320571] = {\"POOL\",1,\"\",\"ALL\",false,false}, --Shadow Well\n    \n    --Stonevault\n    \n    [426283] = {\"ARC\",1,\"\",\"ALL\",true,false}, --Arcing Void\n    [426308] = {\"CURSE\",1,\"\",\"ALL\",true,false}, --Void Infection\n    [449455] = {\"FEAR\",0,\"\",\"ALL\",false,true}, --Howling Fear\n    [429545] = {\"GEAR\",0,\"\",\"ALL\",true,true}, --Censoring Gear\n    [445207] = {\"WAIL\",0,\"\",\"ALL\",false,true}, --Piercing Wail\n    [447141] = {\"LEAP\",2,\"\",\"ALL\",false,false}, --Pulverizing Pounce\n    --[427382] = {\"BUSTER\",2,\"\",\"ALL\",false,false}, --Concussive Smash (tank)\n    [429110] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Alloy Bolt\n    --[429422] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Stone Bolt (tank)\n    [429427] = {\"TOTEM\",2,\"\",\"ALL\",false,true}, --Earth Burst Totem\n    [429109] = {\"HEAL\",0,\"\",\"ALL\",false,true}, --Restoring Metals\n    \n    --City of Threads\n    \n    [443427] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Web Bolt\n    [434122] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Void Bolt\n    [440160] = {\"BOLT\",1,\"\",\"ALL\",true,false}, --Poison Bolt\n    [443430] = {\"BIND\",0,\"\",\"ALL\",true,true}, --Silk Binding\n    --[443397] = {\"BUSTER\",2,\"\",\"ALL\",true,false}, --Venom Strike (tank)\n    [443433] = {\"TWIST\",0,\"\",\"ALL\",true,true}, --Twist Thoughts\n    [448030] = {\"THROW\",2,\"\",\"ALL\",true,false}, --Knife Throw\n    [442536] = {\"BLAST\",1,\"\",\"ALL\",true,true}, --Grimweave Blast\n    [452162] = {\"HEAL\",0,\"\",\"ALL\",false,true}, --Mending Web\n    [446086] = {\"KNOCK\",1,\"\",\"ALL\",false,true}, --Void Wave\n    --[451238] = {\"BUSTER\",2,\"\",\"ALL\",true,false}, --Brutal Jab (tank)\n    [453840] = {\"ALARM\",0,\"\",\"ALL\",false,true}, --Awakening Calling\n    \n    \n}\n\n-- this is only for spells where the mobs cast it for roleplay purposes and they should only show if target is in party\naura_env.rpspellIDs = {\n    [431303] = true, --Night Bolt\n    [431333] = true, --Tormenting Beam\n    [431637] = true, --Umbral Rush\n    [326319] = true, --Spirit bolt\n}\n\n--[[\ndefault logic:\n- category 0 is for important interruptible casts\n- category 1 is for interruptible casts\n- category 2 is for ccable and not interruptible casts\n]]\n\naura_env.getCastInfo = function(unit)\n    local name, startC, endC, icon, notInterruptible, spellID, duration, expirationTime, _, castType\n    if UnitCastingInfo(unit) then\n        name, _, icon, startC, endC, _, _, notInterruptible, spellID = UnitCastingInfo(unit)\n        castType = \"cast\"\n    elseif UnitChannelInfo(unit) then\n        name, _, icon, startC, endC, _, notInterruptible, spellID = UnitChannelInfo(unit)\n        castType = \"channel\"\n    end\n    if startC and endC then\n        duration = (endC - startC) / 1000\n        expirationTime = endC / 1000\n    end\n    return name, duration, expirationTime, icon, notInterruptible, spellID, castType\nend",
