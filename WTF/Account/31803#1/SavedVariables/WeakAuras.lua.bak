["custom"] = "function(allstates, event, ...)\n    if event == \"COMBAT_LOG_EVENT_UNFILTERED\" then\n        local timestamp, subEvent, hideCaster, sourceGUID, sourceName, sourceFlags, sourceRaidFlags, destGUID, destName, destFlags, destRaidFlags, spellID, _,_, auraType = ...\n        if spellID == 448561 then\n            if subEvent == \"SPELL_AURA_APPLIED\" then\n                if not aura_env.flag then\n                    aura_env.flag = true\n                    C_Timer.After(0.1, function() WeakAuras.ScanEvents(\"CAUSESE_SoD_DISPEL\", \"nilcheck\") end)\n                end\n                local unit = UnitTokenFromGUID(destGUID) \n                if unit then\n                    local duration = select(5, WA_GetUnitDebuff(unit, spellID))\n                    if duration then\n                        local specID = WeakAuras.SpecForUnit(unit)\n                        if specID then\n                            table.insert(aura_env.debuffedUnits, {unit = unit, duration = duration, specID = specID, guid = destGUID})\n                        end\n                    end\n                end\n            elseif subEvent == \"SPELL_AURA_REMOVED\" then\n                local state = allstates[destGUID]\n                if state then\n                    state.show = false\n                    state.changed = true\n                    return true\n                end\n            end\n        end\n    elseif event == \"CAUSESE_SoD_DISPEL\" and ... then\n        table.sort(aura_env.debuffedUnits, function(a, b)\n                if a.duration == b.duration then\n                    -- If duration is the same\n                    local prioA = aura_env.specPrio[a.specID] or 0\n                    local prioB = aura_env.specPrio[b.specID] or 0\n                    if prioA == prioB then\n                        -- If specIDs are the same, sort by GUID\n                        return a.guid < b.guid\n                    else\n                        -- Sort by specID priority\n                        return prioA < prioB\n                    end\n                else\n                    -- Sort by duration (highest is dispelled)\n                    return a.duration > b.duration\n                end\n        end)\n        local highestPrio = aura_env.debuffedUnits[1] and aura_env.debuffedUnits[1].unit\n        if highestPrio then\n            allstates[UnitGUID(highestPrio)] = {\n                show = true,\n                changed = true,\n                unit = highestPrio,\n            }\n        end\n        aura_env.flag = false\n        aura_env.debuffedUnits = {}\n        return true\n    elseif event == \"ENCOUNTER_START\" then\n        aura_env.flag = false\n        aura_env.debuffedUnits = {}\n    end\nend",
